{
  "version": 2,
  "buildCommand": "cd frontend && npm run build",
  "devCommand": "cd frontend && npm run dev",
  "installCommand": "npm install && cd frontend && npm install",
  "outputDirectory": "frontend/dist",
  "functions": {
    "api/*.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api/health"
    },
    {
      "src": "/api/analyze",
      "dest": "/api/analyze"
    },
    {
      "src": "/api/test",
      "dest": "/api/test"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}